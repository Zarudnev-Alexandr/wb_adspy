{
  "manifest_version": 3,
  "name": "wb_adspy",
  "version": "0.1",
  "permissions": [
    "storage",
    "activeTab",
    "tabs",
    "scripting",
    "downloads"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.wildberries.ru/*",
        "*://*.wb.ru/*",
        "*://62.109.3.23/*"
      ],
      "js": [
        "script.js"
      ],
      "css": [
        "style.css"
      ]
    }
  ],
  "background": {
    "service_worker": "bg.js"
  }
}